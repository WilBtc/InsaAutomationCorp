{
  "dashboard": {
    "id": null,
    "uid": "insa-latency",
    "title": "INSA CRM - Latency & Performance",
    "tags": ["insa", "crm", "latency", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "P50 Latency (Median)",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "P95 Latency",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "P99 Latency",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 30, "color": "red"}
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Average Latency",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(insa_agent_request_duration_seconds_sum[5m])) / sum(rate(insa_agent_request_duration_seconds_count[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Latency Percentiles (P50, P95, P99) - All Agents",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "P50 (Median)"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true,
              "axisPlacement": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "P95 Latency by Agent",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (agent_type, le))",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Average Latency by Agent",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(insa_agent_request_duration_seconds_sum[5m])) by (agent_type) / sum(rate(insa_agent_request_duration_seconds_count[5m])) by (agent_type)",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 8,
        "type": "table",
        "title": "Latency Summary by Agent",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (agent_type, le))",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (agent_type, le))",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (agent_type, le))",
            "refId": "C",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(rate(insa_agent_request_duration_seconds_sum[5m])) by (agent_type) / sum(rate(insa_agent_request_duration_seconds_count[5m])) by (agent_type)",
            "refId": "D",
            "format": "table",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "P95",
              "desc": true
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "auto"
            },
            "unit": "s",
            "decimals": 3
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "agent_type"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Agent"
                },
                {
                  "id": "custom.width",
                  "value": 150
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "agent_type"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "agent_type": 0,
                "Value #A": 1,
                "Value #D": 2,
                "Value #B": 3,
                "Value #C": 4
              },
              "renameByName": {
                "agent_type": "Agent",
                "Value #A": "P50",
                "Value #D": "Average",
                "Value #B": "P95",
                "Value #C": "P99"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "type": "heatmap",
        "title": "Latency Distribution Heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (le)",
            "refId": "A",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 2,
          "color": {
            "exponent": 0.5,
            "fill": "dark-blue",
            "mode": "scheme",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Blues",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "showValue": "never",
          "tooltip": {
            "show": true,
            "yHistogram": true
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 10,
        "type": "bargauge",
        "title": "Slowest Operations (P99 by Agent)",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.99, sum(rate(insa_agent_request_duration_seconds_bucket[5m])) by (agent_type, le)))",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {
            "valueSize": 16
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "orange"},
                {"value": 10, "color": "red"}
              ]
            },
            "unit": "s",
            "decimals": 3,
            "min": 0
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Request Size Distribution",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(insa_agent_request_size_bytes_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "P50 Request Size"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_request_size_bytes_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "P95 Request Size"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(insa_agent_request_size_bytes_bucket[5m])) by (le))",
            "refId": "C",
            "legendFormat": "P99 Request Size"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Response Size Distribution",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(insa_agent_response_size_bytes_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "P50 Response Size"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(insa_agent_response_size_bytes_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "P95 Response Size"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(insa_agent_response_size_bytes_bucket[5m])) by (le))",
            "refId": "C",
            "legendFormat": "P99 Response Size"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      }
    ]
  },
  "overwrite": true
}
