{
  "dashboard": {
    "id": null,
    "uid": "insa-requests",
    "title": "INSA CRM - Request Metrics",
    "tags": ["insa", "crm", "requests"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Requests",
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_agent_requests_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Success Rate",
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 0},
        "targets": [
          {
            "expr": "(sum(insa_agent_requests_total{status=\"success\"}) / sum(insa_agent_requests_total)) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Error Rate",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "(sum(insa_agent_requests_total{status=\"error\"}) / sum(insa_agent_requests_total)) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Timeout Rate",
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(sum(insa_agent_requests_total{status=\"timeout\"}) / sum(insa_agent_requests_total)) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 2, "color": "red"}
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Requests/sec (1m)",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(insa_agent_requests_total[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 48
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Request Rate by Agent (Success vs Error)",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(insa_agent_requests_total{status=\"success\"}[1m])",
            "refId": "A",
            "legendFormat": "{{agent_type}} - Success"
          },
          {
            "expr": "rate(insa_agent_requests_total{status=\"error\"}[1m])",
            "refId": "B",
            "legendFormat": "{{agent_type}} - Error"
          },
          {
            "expr": "rate(insa_agent_requests_total{status=\"timeout\"}[1m])",
            "refId": "C",
            "legendFormat": "{{agent_type}} - Timeout"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {
                "mode": "none"
              }
            },
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*Error.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*Timeout.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "piechart",
        "title": "Requests by Agent (Total)",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum by (agent_type) (insa_agent_requests_total)",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "percent"]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "displayLabels": ["percent"]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "type": "table",
        "title": "Top Agents by Volume",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "topk(10, sum by (agent_type) (rate(insa_agent_requests_total[5m])))",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Value",
              "desc": true
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Requests/sec"
                },
                {
                  "id": "unit",
                  "value": "reqps"
                },
                {
                  "id": "decimals",
                  "value": 3
                },
                {
                  "id": "custom.displayMode",
                  "value": "gradient-gauge"
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "agent_type": 0,
                "Value": 1
              },
              "renameByName": {
                "agent_type": "Agent"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "type": "timeseries",
        "title": "Request Rate Trends (1h, 6h, 24h)",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(insa_agent_requests_total[1h]))",
            "refId": "A",
            "legendFormat": "1 hour average"
          },
          {
            "expr": "sum(rate(insa_agent_requests_total[6h]))",
            "refId": "B",
            "legendFormat": "6 hour average"
          },
          {
            "expr": "sum(rate(insa_agent_requests_total[24h]))",
            "refId": "C",
            "legendFormat": "24 hour average"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 5,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 10,
        "type": "heatmap",
        "title": "Request Volume Heatmap (by Agent)",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum by (agent_type) (rate(insa_agent_requests_total[1m]))",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {},
          "cellGap": 2,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "showValue": "never",
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      }
    ]
  },
  "overwrite": true
}
