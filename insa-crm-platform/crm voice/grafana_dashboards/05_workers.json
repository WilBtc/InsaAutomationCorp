{
  "dashboard": {
    "id": null,
    "uid": "insa-workers",
    "title": "INSA CRM - Worker Health & Performance",
    "tags": ["insa", "crm", "workers", "health"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Healthy Workers",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_worker_health_status == 1)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Unhealthy Workers",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_worker_health_status == 0)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Active Requests",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_worker_active_requests)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 20, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total Queue Depth",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_worker_queue_size)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Worker Health Status Over Time",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "insa_worker_health_status",
            "refId": "A",
            "legendFormat": "{{worker_name}} ({{worker_type}})"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "lineWidth": 3,
              "fillOpacity": 30,
              "showPoints": "always",
              "spanNulls": false
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "UNHEALTHY"},
              {"type": "value", "value": "1", "text": "HEALTHY"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "short",
            "min": -0.1,
            "max": 1.1
          }
        }
      },
      {
        "id": 6,
        "type": "state-timeline",
        "title": "Worker Health Timeline",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "insa_worker_health_status",
            "refId": "A",
            "legendFormat": "{{worker_name}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 0,
              "fillOpacity": 80
            },
            "mappings": [
              {
                "type": "value",
                "value": "0",
                "text": "UNHEALTHY",
                "color": "red"
              },
              {
                "type": "value",
                "value": "1",
                "text": "HEALTHY",
                "color": "green"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "type": "bargauge",
        "title": "Active Requests by Worker",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "insa_worker_active_requests",
            "refId": "A",
            "legendFormat": "{{worker_name}}",
            "instant": true
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {
            "valueSize": 20
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "orange"},
                {"value": 20, "color": "red"}
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 30
          }
        }
      },
      {
        "id": 8,
        "type": "bargauge",
        "title": "Queue Size by Worker",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "insa_worker_queue_size",
            "refId": "A",
            "legendFormat": "{{worker_name}}",
            "instant": true
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {
            "valueSize": 20
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "orange"},
                {"value": 100, "color": "red"}
              ]
            },
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "Total Worker Restarts",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12},
        "targets": [
          {
            "expr": "sum(insa_worker_restarts_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "orange"},
                {"value": 10, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Active Requests Over Time",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "insa_worker_active_requests",
            "refId": "A",
            "legendFormat": "{{worker_name}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Queue Size Over Time",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "insa_worker_queue_size",
            "refId": "A",
            "legendFormat": "{{worker_name}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Worker Restarts by Reason",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(insa_worker_restarts_total[5m])",
            "refId": "A",
            "legendFormat": "{{worker_name}} - {{reason}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "spanNulls": true,
              "stacking": {
                "mode": "normal"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 13,
        "type": "table",
        "title": "Worker Status Summary",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "insa_worker_health_status",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "insa_worker_active_requests",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "insa_worker_queue_size",
            "refId": "C",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(insa_worker_restarts_total) by (worker_name)",
            "refId": "D",
            "format": "table",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Health",
              "desc": false
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "worker_name"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Worker"
                },
                {
                  "id": "custom.width",
                  "value": 200
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Health"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {"type": "value", "value": "0", "text": "UNHEALTHY", "color": "red"},
                    {"type": "value", "value": "1", "text": "HEALTHY", "color": "green"}
                  ]
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "worker_name"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "worker_type": true
              },
              "indexByName": {
                "worker_name": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3,
                "Value #D": 4
              },
              "renameByName": {
                "worker_name": "Worker",
                "Value #A": "Health",
                "Value #B": "Active Requests",
                "Value #C": "Queue Size",
                "Value #D": "Restarts"
              }
            }
          }
        ]
      }
    ]
  },
  "overwrite": true
}
