{
  "dashboard": {
    "id": null,
    "uid": "insa-cache",
    "title": "INSA CRM - Cache Performance",
    "tags": ["insa", "crm", "cache", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Overall Cache Hit Rate",
        "gridPos": {"h": 5, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "(sum(rate(insa_cache_hits_total[5m])) / (sum(rate(insa_cache_hits_total[5m])) + sum(rate(insa_cache_misses_total[5m])))) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Cache Hits",
        "gridPos": {"h": 5, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_cache_hits_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Cache Misses",
        "gridPos": {"h": 5, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_cache_misses_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "orange"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total Cache Size",
        "gridPos": {"h": 5, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(insa_cache_size_entries)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Cache Hit Rate by Cache Type",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
        "targets": [
          {
            "expr": "(rate(insa_cache_hits_total[1m]) / (rate(insa_cache_hits_total[1m]) + rate(insa_cache_misses_total[1m]))) * 100",
            "refId": "A",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "min"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 15,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Cache Hits vs Misses",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
        "targets": [
          {
            "expr": "rate(insa_cache_hits_total[1m])",
            "refId": "A",
            "legendFormat": "{{cache_type}} - Hits"
          },
          {
            "expr": "rate(insa_cache_misses_total[1m])",
            "refId": "B",
            "legendFormat": "{{cache_type}} - Misses"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {
                "mode": "none"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*Hits.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*Misses.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "bargauge",
        "title": "Cache Hit Rate by Type (%)",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 13},
        "targets": [
          {
            "expr": "(sum(insa_cache_hits_total) by (cache_type) / (sum(insa_cache_hits_total) by (cache_type) + sum(insa_cache_misses_total) by (cache_type))) * 100",
            "refId": "A",
            "legendFormat": "{{cache_type}}",
            "instant": true
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {
            "valueSize": 18
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            },
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 8,
        "type": "bargauge",
        "title": "Cache Size by Type (entries)",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 13},
        "targets": [
          {
            "expr": "insa_cache_size_entries",
            "refId": "A",
            "legendFormat": "{{cache_type}}",
            "instant": true
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {
            "valueSize": 18
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 100, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 10000, "color": "red"}
              ]
            },
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 9,
        "type": "piechart",
        "title": "Cache Operations Distribution",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 13},
        "targets": [
          {
            "expr": "sum(insa_cache_hits_total) by (cache_type)",
            "refId": "A",
            "legendFormat": "{{cache_type}} - Hits"
          },
          {
            "expr": "sum(insa_cache_misses_total) by (cache_type)",
            "refId": "B",
            "legendFormat": "{{cache_type}} - Misses"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "percent"]
          },
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "displayLabels": ["percent"]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Cache Evictions by Reason",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 21},
        "targets": [
          {
            "expr": "rate(insa_cache_evictions_total[1m])",
            "refId": "A",
            "legendFormat": "{{cache_type}} - {{reason}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {
                "mode": "normal"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Cache Size Over Time",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 21},
        "targets": [
          {
            "expr": "insa_cache_size_entries",
            "refId": "A",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 12,
        "type": "table",
        "title": "Cache Performance Summary",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 29},
        "targets": [
          {
            "expr": "sum(insa_cache_hits_total) by (cache_type)",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(insa_cache_misses_total) by (cache_type)",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "(sum(insa_cache_hits_total) by (cache_type) / (sum(insa_cache_hits_total) by (cache_type) + sum(insa_cache_misses_total) by (cache_type))) * 100",
            "refId": "C",
            "format": "table",
            "instant": true
          },
          {
            "expr": "insa_cache_size_entries",
            "refId": "D",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(insa_cache_evictions_total) by (cache_type)",
            "refId": "E",
            "format": "table",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Hit Rate %",
              "desc": true
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "cache_type"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Cache Type"
                },
                {
                  "id": "custom.width",
                  "value": 150
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "cache_type"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "cache_type": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3,
                "Value #D": 4,
                "Value #E": 5
              },
              "renameByName": {
                "cache_type": "Cache Type",
                "Value #A": "Hits",
                "Value #B": "Misses",
                "Value #C": "Hit Rate %",
                "Value #D": "Size (entries)",
                "Value #E": "Evictions"
              }
            }
          }
        ]
      }
    ]
  },
  "overwrite": true
}
